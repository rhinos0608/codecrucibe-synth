{
  "name": "codecrucible-rust-executor",
  "version": "4.2.4",
  "description": "High-performance Rust executor for CodeCrucible with cross-platform NAPI bindings",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "binaryName": "codecrucible-rust-executor",
    "targets": [
      "x86_64-pc-windows-msvc",
      "x86_64-apple-darwin",
      "x86_64-unknown-linux-gnu",
      "aarch64-apple-darwin",
      "aarch64-unknown-linux-gnu",
      "aarch64-pc-windows-msvc",
      "x86_64-unknown-linux-musl",
      "aarch64-unknown-linux-musl",
      "x86_64-unknown-freebsd",
      "armv7-unknown-linux-gnueabihf",
      "i686-pc-windows-msvc",
      "i686-unknown-linux-gnu"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rhinos0608/codecrucible-synth.git",
    "directory": "rust-executor"
  },
  "scripts": {
    "build": "napi build --release",
    "build:debug": "napi build",
    "prepublishOnly": "napi prepublish -t npm --skip-gh-release",
    "artifacts": "napi artifacts",
    "version": "napi version",
    "test": "cargo test"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.18.4"
  },
  "dependencies": {
    "@node-rs/helper": "^1.3.0"
  },
  "packageManager": "yarn@1.22.19",
  "files": [
    "*.node"
  ],
  "engines": {
    "node": ">= 18.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32",
    "freebsd"
  ],
  "cpu": [
    "x64",
    "arm64",
    "ia32",
    "arm"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}