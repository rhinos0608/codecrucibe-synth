# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/codecrucible

# Authentication
SESSION_SECRET=your-session-secret-here

# OpenAI Dev Proxy - For unlimited GPT-4/3.5 generations in development
OPENAI_API_KEY=your-openai-api-key-here

# Stripe Configuration (Optional - for payment processing)
STRIPE_SECRET_KEY=sk_test_your-stripe-secret-key
STRIPE_WEBHOOK_SECRET=whsec_your-webhook-secret
STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-publishable-key

# Development Mode Override (Optional)
DEV_MODE=true

# Production Deployment Override - ProductLaunch Configuration
# Set to 'false' to re-enable dev mode features in development
FORCE_PRODUCTION_MODE=true

# Environment Configuration
NODE_ENV=development